{
    "name": "gpenverne/putio-drive-sdk",
    "type": "bundle",
    "description": "A bundle for explore putio files",
    "keywords": ["putio", "api"],
    "license": "MIT",
    "authors": [{
        "name": "Gpenverne",
        "homepage": "https://github.com/gpenverne"
    }],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.4",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/class-loader": "*",
        "symfony/dependency-injection": "*",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gpenverne\\PutioDriveBundle\\": "",
            "": "vendor/"
        }
    }
}
