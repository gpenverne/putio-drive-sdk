{
    "name": "gpenverne/putio-drive-sdk",
    "type": "bundle",
    "description": "A bundle for explore putio files",
    "keywords": ["putio", "api"],
    "license": "MIT",
    "authors": [{
        "name": "Gpenverne",
        "homepage": "https://github.com/gpenverne"
    }],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/class-loader": "*",
        "symfony/dependency-injection": "*",
        "nicoswd/putio": "*",
        "gpenverne/psr-cloud-files": "dev-master",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "henrikbjorn/phpspec-code-coverage": "^3.0",
        "friendsofphp/php-cs-fixer": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gpenverne\\PutioDriveBundle\\": "src",
            "": "vendor/"
        }
    }
}
